# GitHub Actions Testing Environment Variables
# Copy this file to .env and fill in the values for testing GitHub Actions locally with act

# GitHub tokens
GITHUB_TOKEN=your_github_token_here

# CodeQL analysis tokens
SONAR_TOKEN=your_sonar_token_here

# NPM registry tokens
NPM_TOKEN=your_npm_token_here

# Security scanning tokens
SNYK_TOKEN=your_snyk_token_here
CODECOV_TOKEN=your_codecov_token_here

# Docker registry credentials (if needed)
DOCKER_USERNAME=your_docker_username
DOCKER_PASSWORD=your_docker_password

# AWS credentials (if using AWS services)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# Application-specific environment variables
DEBUG=true
NODE_ENV=test
LOG_LEVEL=debug
LOG_DIR=./logs 